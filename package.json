{
  "name": "grpc_app",
  "version": "1.0.0",
  "description": "gRPC aplication",
  "main": "./dist/bin/www",
  "scripts": {
    "start": "node server.js",
    "lint": "standard --fix",
    "postinstall": "npm run clean && npm run transpile",
    "watch": "npm-run-all clean transpile && nodemon ./dist-server/",
    "start:stream": "node ./dist-server",
    "transpile": "babel ./src/**.js --out-dir dist-server",
    "clean": "rimraf dist-server",
    "build": "npm-run-all clean transpile"
  },
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IsaacNav/gRPC.git"
  },
  "author": "IsaacNav",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/IsaacNav/gRPC/issues"
  },
  "homepage": "https://github.com/IsaacNav/gRPC#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard": "16.0.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.3.7",
    "apollo-server": "^3.3.0",
    "apollo-server-fastify": "^3.3.0",
    "csv-parser": "3.0.0",
    "event-stream": "4.0.1",
    "fastify": "^3.21.6",
    "graphql": "^15.6.0",
    "mercurius": "^8.4.2",
    "pino": "^6.13.3",
    "pino-pretty": "^7.0.1",
    "superagent": "6.1.0"
  }
}
